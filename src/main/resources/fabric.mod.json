{
  "schemaVersion": 1,
  "id": "soaring-phantoms",
  "version": "${version}",
  "name": "Soaring Phantoms",
  "description": "Spawn phantoms above a certain altitude, with a chance increasing the higher up you are. Fully configurable.",
  "authors": [
    "pajic"
  ],
  "contact": {
    "website": "https://modrinth.com/project/soaring-phantoms",
    "repo": "https://github.com/pajicadvance/soaring-phantoms"
  },
  "license": "MIT",
  "icon": "soaring-phantoms.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.pajic.soaringphantoms.Main"
    ]
  },
  "mixins": [
    "soaring-phantoms.mixins.json"
  ],
  "accessWidener": "soaring-phantoms.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
